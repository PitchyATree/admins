game.StarterGui:SetCore("SendNotification", {
	Title = "HOODWINK";
	Text = "Prefix is :";
})

game.StarterGui:SetCore("ChatMakeSystemMessage", {
    Text = "Thank you for using Hoodwink! Press ; to see commands.";
    Font = Enum.Font.SourceSansBold;
    Color = Color3.fromRGB(255, 0, 255);
})

