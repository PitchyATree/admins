				velocity.TargetOffset = pos
				velocity:fire()
				local b = Instance.new("BodyGyro", limb)
			end
			while wait() do
				doo(lplr.Character["LeftUpperArm"], Vector3.new(-0.5,-2,-1))
				doo(lplr.Character["RightUpperArm"], Vector3.new(0.5,-2,-1))
				doo(lplr.Character["LeftUpperLeg"], Vector3.new(-0.5,-2,1))
