HOODWINK.TextWrapped = true

for i = 1,0,-0.1 do
	HOODWINK.TextTransparency = i
	W.BackgroundTransparency = i
	W.TextTransparency = i
	H.BackgroundTransparency = i
	H.TextTransparency = i
	wait()
end

wait(2)

for i = 0,1,0.1 do
	HOODWINK.TextTransparency = i
	W.BackgroundTransparency = i
	W.TextTransparency = i
	H.BackgroundTransparency = i
	H.TextTransparency = i
	wait()
end

HWIntro:Destroy()

